{
  "name": "yiitk/yiitk-nox-media-manager",
  "description": "YiiTK - NOX Media Manager for Yii Framework version 2",
  "type": "yii2-extension",
  "keywords": ["yii2", "extension", "framework", "php"],
  "license": "BSD-3-Clause",
  "version": "0.0.1",
  "authors": [
    {
      "name": "YiiTK",
      "email": "yiitk@jsas.com.br"
    },
    {
      "name": "Jonatas Sas",
      "email": "atendimento@jsas.com.br"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0.0",
    "ext-json": "*",
    "ext-curl": "*",

    "yiisoft/yii2": "~2.0.15",

    "yiisoft/yii2-bootstrap4": "~2.0.0"
  },
  "require-dev": {
    "yiisoft/yii2-faker": "~2.0.0",

    "codeception/base": "^2.2.3",
    "codeception/verify": "~0.3.1"
  },
  "autoload": {
    "psr-4": {
      "yiitk\\media\\": "src"
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/yiitk/yiitk-nox-media-manager"
    }
  ]
}
